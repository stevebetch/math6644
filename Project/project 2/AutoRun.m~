%% This is the auto running script for Project 2

atol=1e-6;
rtol=1e-6;
maxIt=10000;
x0=ones(200,1);
fhandle=@(x) Eqn(x);
m=2;

Fstart=tic;
[ Fx, Fcount, Ferr ] = FixedPoint( fhandle, x0,atol, rtol,maxIt);
fend=toc(Fstart);

cstart=tic;
[Cx, Ccount, Cerr ] = Chord( fhandle, x0,atol, rtol,maxIt);
cend=toc(cstart);

nstart=tic;
[Nx,Ncount, Nerr]=Newton( fhandle, x0,atol, rtol,maxIt);
nend=toc(nstart);

shstart=tic;
[Shx,Shcount, Sherr]=Shamanskii( fhandle, x0,m,atol, rtol,maxIt);
shend=toc(shstart);





%% Ploting section figure 1
figure;

semilogy(1:Fcount,Ferr,'r--*','DisplayName','Fixed Point Error')
hold on; 
semilogy(1:Ccount,Cerr,'c--^','DisplayName','Chord Method Error');
semilogy(1:Ncount,Nerr,'b--d','DisplayName','Newton''s Method Error');
semilogy(1:Shcount,Sherr,'k--*','DisplayName','Shamanskii''s Method Error');


% Create ylabel
ylabel({'Relative error of the function'},'FontSize',20,'Interpreter','latex');

% Create xlabel
xlabel({'Iteration number'},'FontSize',20,'Interpreter','latex');

% Create title
title({'Function a Start and End Points'},'FontSize',20,...
    'Interpreter','latex');

% Create legend
legend1 = legend('show');
set(legend1,'Interpreter','latex','FontSize',16);

hold off;



%% Latex out
fprintf('function  &Iterations Required to converge & Time to Converge & Final Error %s\n','\\\hline')
fprintf('Fixed Point&%f& %f& %f%s \n',Fcount,fend,Ferr(end),'\\\hline')
fprintf('Chord&%f& %f& %f%s \n',Ccount,cend,Cerr(end),'\\\hline')
fprintf('Newton''s Method&%f& %.8f& %.8f%s\n',Ncount,nend,Nerr(end),'\\\hline')
fprintf('Shamanskii''s Method&%f& %.8f& %.8f%s \n',Shcount,shend,Sherr(end),'\\\hline')
fprintf('\n');

